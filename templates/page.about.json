    "23da5fd2-5422-48b5-9af7-e5e51b7521f9": {
      "type": "bfj-halved-blocks",
      "blocks": {
        "template--15482710851684__23da5fd2-5422-48b5-9af7-e5e51b7521f9-166555404973f38c11-1": {
          "type": "image",
          "settings": {
            "heading": "",
            "image": "shopify:\/\/shop_images\/img_19b306dc-7bde-4638-9314-65dbe203c5a7.png",
